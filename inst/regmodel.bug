var beta.t[J,2], beta_s[J], beta_y[J], muu[N,2];

model {


  for(i in 1:J) {

    beta_s[i] <- beta.t[i,1]
    beta_y[i] <- beta.t[i,2]
    beta.t[i,1:2] ~ dmnorm.vcov(prior_mu[i,1:2], prior_sig[1:2,1:2,i])

  }

  muu[,1] <- X %*% beta_s
  muu[,2] <- X %*% beta_y

  for(i in 1:N){

    sy[i, 1:2] ~ dmnorm(muu[i,1:2], tau)

  }

  tau ~ dscaled.wishart(omega, 4)

}
